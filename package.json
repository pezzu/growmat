{
  "name": "growmat",
  "version": "1.0.0",
  "description": "Lorem ipsum",
  "author": "peter.sukhenko@gmail.com",
  "main": "growmat.js",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=1.4.28"
  },
  "scripts": {
    "deploy": "pm2 deploy ecosystem.json production",
    "monitor": "nodemon --ignore services/growmat/public/ --ignore services/live-stream services/growmat/growmat.js 8080",
    "postinstall": "bower install --config.interactive=false && node conf/pi-install.js",
    "restart": "pm2 restart ecosystem.json --env production",
    "start": "pm2 start ecosystem.json --env production",
    "stop": "pm2 stop ecosystem.json"
  },
  "repository": {
    "type": "git",
    "url": "https://pesu@bitbucket.org/pesu/growmat"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "bower": "^1.8.0",
    "detect-rpi": "^1.2.0",
    "express": "^4.15.3",
    "node-schedule": "^1.2.3",
    "pm2": "^2.6.1",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  },
  "piDependencies": {
    "node-dht-sensor": "^0.0.32",
    "rpi-gpio": "^0.8.1"
  }
}
